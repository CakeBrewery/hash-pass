{
  "manifest_version": 2,
  "name": "Hash Pass"
  "version": "1",
  "browser_action": {
    "default_title": "Hash Pass",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
